apiVersion: security-profiles-operator.x-k8s.io/v1alpha1
kind: AppArmorProfile
metadata:
  creationTimestamp: null
  name: demobinary
spec:
  abstract:
    capability:
      allowedCapabilities:
      - net_admin
    executable: {}
    filesystem:
      readOnlyPaths:
      - /proc/@{pid}/sys/net/core/somaxconn
    network:
      allowedProtocols:
        allowTcp: true
status: {}

